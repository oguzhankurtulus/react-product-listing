{
  "name": "groceryapp",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.*.*"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "env-cmd -f environments/.env.development npm run start",
    "build": "react-scripts build",
    "build:prod": "env-cmd -f environments/.env.prod npm run-script build",
    "test": "jest --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier --write .ts,.tsx,.js,.jsx,.json .",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "prepare": "husky install",
    "svgr": "svgr ./src/assets/icons -d ./src/components/Icons --icon"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "node_modules/.bin/prettierÂ --write",
      "git add"
    ],
    "/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm run tsc"
    }
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "7.12.13",
    "@babel/plugin-transform-modules-commonjs": "7.14.0",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/plugin-transform-typescript": "7.13.0",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "7.14.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@loadable/component": "5.14.1",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.8",
    "@types/node": "15.0.2",
    "@types/react": "17.0.5",
    "@types/react-router-dom": "5.1.7",
    "antd": "4.15.4",
    "axios": "0.21.1",
    "babel-plugin-import": "1.13.3",
    "customize-cra": "1.0.0",
    "env-cmd": "10.1.0",
    "i18next": "20.2.2",
    "jest": "26.6.3",
    "query-string": "7.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-i18next": "11.8.15",
    "react-jss": "10.8.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "4.1.0",
    "redux-thunk": "2.3.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.16",
    "@babel/core": "7.14.0",
    "@babel/node": "7.13.13",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/preset-env": "7.14.1",
    "@svgr/cli": "^5.5.0",
    "@types/jest": "26.0.23",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "babel": "6.23.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-preset-env": "1.7.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0"
  }
}
